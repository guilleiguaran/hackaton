{
  "author"        : {"name": "Kevin Malakoff", "url": "https://github.com/kmalakoff"},
  "name"          : "knockback",
  "version"       : "0.18.4",
  "description"   : "Knockback.js provides Knockout.js magic for Backbone.js Models and Collections",
  "keywords"      : ["knockback", "knockbackjs", "backbone", "backbonejs", "knockout", "knockoutjs"],
  "main"          : "knockback.js",
  "private"       : true,

  "url"           : "http://kmalakoff.github.com/knockback/",
  "homepage"      : "http://kmalakoff.github.com/knockback/",
  "repository"    : {"type": "git", "url": "http://github.com/kmalakoff/knockback.git"},

  "scripts": {
    "postinstall": "./node_modules/.bin/bake postinstall",
    "clean": "./node_modules/.bin/bake clean",
    "build": "./node_modules/.bin/bake build",
    "watch": "./node_modules/.bin/bake watch",
    "test": "./node_modules/.bin/bake test -c",
    "docs": "./node_modules/.bin/codo -n Knockback.js -o doc src/."
  },
  "dependencies": {
    "underscore": ">=1.1.7",
    "backbone": ">=0.5.1",
    "knockout": ">=1.2.1"
  },
  "devDependencies": {
    "coffee-script": "*",
    "easy-bake": "*",
    "codo": "2.x.x",
    "phantomjs" : "*",
    "uglify-js" : "*",
    "module-bundler" : "*"
  },
  "optionalDependencies": {
    "backbone-relational": "0.8.x",
    "backbone-modelref": "0.1.x",
    "lodash": "2.x.x",

    "backbone-orm": "0.5.x",
    "moment": "2.x.x",

    "backbone-associations": "0.5.x",
    "supermodel": "0.0.x"
  },
  "jam": {
    "dependencies": {
      "underscore": ">=1.1.7",
      "backbone": ">=0.5.1",
      "knockout": ">=1.2.1"
    }
  }
}
